generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Document {
  id        String   @id @default(uuid())
  title     String   @default("Untitled Document")
  content   Bytes
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
